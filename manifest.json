{
    "manifest_version": 3,
    "name": "YOLO Image Annotator",
    "version": "1.0",
    "description": "Annotate images using YOLO and TensorFlow.js",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup/popup.html"
    },
    "background": {
      "service_worker": "scripts/yolo.js"
    }
  }
  